    <plugin name="ThrusterPlugin" filename="libusv_gazebo_thrust_plugin.so">
      <bodyName>body</bodyName>
      <mappingType>1</mappingType> <!-- 0 is liner, 1 is nonliner -->
      <boatArea>96.0</boatArea>
    	<boatWidth>12.0</boatWidth>
    	<boatLength>8.0</boatLength>
    	<thrustOffsetZ>-0.05</thrustOffsetZ>
    	<!-- Thrust specification -->
    	<maxCmd>100.0</maxCmd>
    	<maxForceFwd>25.0</maxForceFwd>
    	<maxForceRev>-25.0</maxForceRev>
    	<cmdTimeout>0.1</cmdTimeout>
    </plugin>

