<?xml version='1.0'?>
<sdf version='1.4'>
  <model name="tug_boat">
    <pose>0 0 0 0 0 0</pose>
    <static>false</static>

    <!-- LINK -->
    <link name="body">
      <pose>0 0 0 0 0 0</pose>

      <collision name="body">
        <geometry>
          <box><size>12.0 8.0 1.0</size></box>
        </geometry>
      </collision>

      <visual name="body">
        <geometry>
          <box><size>12.0 8.0 1.0</size></box>
        </geometry>

        <material>
          <diffuse>0.4 0.4 0 1</diffuse>
        </material>
      </visual>

      <inertial>
        <mass> 1000 </mass>
      </inertial>
    </link>

    <link name="right_floater">
      <pose>0 -3 -1 0 1.57 0</pose>
      <collision name="right_floater">
        <geometry>
          <cylinder>
            <radius>1.0</radius>
            <length>12.0</length>
          </cylinder>
        </geometry>
      </collision>

      <visual name="right_floater">
        <geometry>
          <cylinder>
            <radius>1.0</radius>
